{
  "statsd": {
    "prefix": "{{statsd.prefix}}",
    "url": "udp://{{statsd.host}}:{{statsd.port}}"
  },

  "influx": {
    "connectionString": "https://{{influxdb.username}}:{{influxdb.password}}@{{influxdb.host}}:{{influxdb.port}}/db/{{influxdb.database}}"
  },

  "taskcluster": {
    "clientId": "{{taskcluster.clientId}}",
    "accessToken": "{{taskcluster.accessToken}}"
  },

  "pulse": {
    "username": "{{pulse.username}}",
    "password": "{{pulse.password}}"
  },

  "testdroid": {
    "url": "{{testdroid.url}}",
    "username": "{{testdroid.username}}",
    "password": "{{testdroid.password}}"
  },

  "statelessHostname": {
    "secret": "{{statelessHostname.secret}}"
  },

  "features": {
    "relengAPIProxy": {
      "token": "{{relengAPIToken}}"
    }
  }
}
